{
  "name": "vim-tab-chrome",
  "version": "0.0.1",
  "description": "Opens up a local vim editor in browser",
  "icons": {
    "128": "images/icon_128.png",
    "16": "images/icon.png",
    "48": "images/icon_48.png"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "vim",
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "tabs"
  ],
  "manifest_version": 2
}
